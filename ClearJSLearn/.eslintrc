{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-undef": "off",
    "space-in-parens": "off",
    "strict": "off",
    // "no-console": "error",
    "no-console": "off",
    // "semi": "off"
    "no-plusplus": "off",
    // "eol-last": "off",
    "max-len": ["error", 150, {"ignoreUrls": true}],
    // "no-tabs": "off",
    "no-alert": "off",
    // "prefer-destructuring": "off",
    // "indent": [2, "tab"]
    // "prefer-const": "off"
    "max-classes-per-file": "off",
    "no-restricted-syntax": "off",
    // "max-len": "off",
    "consistent-return": "off"
  }
}
